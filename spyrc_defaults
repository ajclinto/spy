map j down
map k up
map h left
map l right

map KEY_DOWN down
map KEY_UP up
map KEY_LEFT left
map KEY_RIGHT right

map d display
map v enter
map u climb

map r pagedown
map t pageup
map KEY_NPAGE pagedown
map KEY_PPAGE pageup

map KEY_HOME firstfile
map KEY_END lastfile
map G lastfile

map q quit
map ' loadrc

map g jump

map / search
map n next

map ! unix_cmd
map : unix_cmd
map ; unix_cmd

map . last_cmd

map % unix ./%

map L unix ls -l %

