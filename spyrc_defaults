map j down
map k up
map h left
map l right

map KEY_DOWN down
map KEY_UP up
map KEY_LEFT left
map KEY_RIGHT right

map d display
map v enter
map u climb

map ^F pagedown
map ^B pageup
map KEY_NPAGE pagedown
map KEY_PPAGE pageup

map KEY_HOME firstfile
map KEY_END lastfile
map G lastfile

map ^D quit
map q quit
map ' loadrc
map KEY_ENTER redraw
map KEY_RESIZE redraw

map g jump
map J jump

map / search
map n next
map N prev

map ! unix_cmd
map : unix_cmd
map ; unix_cmd

map . last_cmd

map % unix ./%

map L unix ls -l %

map KEY_BACKSPACE ignore
map <Space> ignore
